<?xml version="1.0" encoding="ISO-8859-1"?>
<odoo>
    <data>
        <template id="envio_dte_cert"  inherit_id="l10n_cl_edi.envio_dte">
            <xpath expr="//SubTotDTE" position="replace">
            <SubTotDTE t-foreach="tipodte_subtotals" t-as="subtotal">
                <TpoDTE t-esc="subtotal.code"/>
                <NroDTE t-esc="subtotal.count"/>
            </SubTotDTE>
            </xpath>
            <xpath expr="//SetDTE/t[@t-esc='dte']" position="replace">
                <t t-foreach="dtes" t-as="dte">
        <t t-esc="dte"/>
                </t>
            </xpath>
        </template>
